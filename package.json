{
  "name": "flo-public-gateway",
  "version": "1.0.0",
  "description": "Flo Public Gateway",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/flotechnologies/flo-public-gateway"
  },
  "main": "app.js",
  "scripts": {
    "copy-docs": "shx rm -rf dist/docs/*.yaml dist/docs/*/ && copyfiles docs/{,**/}*.yaml dist",
    "build": "rm -rf ./dist && npm run build-ts && npm run tslint && npm run copy-docs",
    "build-ts": "tsc",
    "debug": "npm run build && node --inspect=0.0.0.0 dist/app.js",
    "serve": "npm run build && node dist/app.js",
    "start": "node dist/app.js",
    "test": "jest --colors --coverage --verbose",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch": "npm run copy-docs && tsc-watch --onSuccess 'node ./dist/app.js' --onFailure 'echo Compilation Failed'",
    "watch-debug": "npm run copy-docs && tsc-watch --onSuccess 'node --inspect=0.0.0.0 ./dist/app.js' --onFailure 'echo Compilation Failed'",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "dev:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json src/get-device.ts",
    "dev": "ts-node-dev --transpile-only --respawn --project tsconfig.json src/get-device.ts",
    "floenv": "zsh -i -c \"floenv prod\""
  },
  "author": "Flo Technologies",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.35",
    "@instana/collector": "2.30.0",
    "@instana/core": "2.30.0",
    "agentkeepalive": "^4.5.0",
    "aws-sdk": "^2.1438.0",
    "axios": "^1.4.0",
    "axios-better-stacktrace": "^2.1.5",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "fast-json-stable-stringify": "^2.1.0",
    "fp-ts": "^2.16.1",
    "fp-ts-contrib": "^0.1.29",
    "glob": "^10.3.3",
    "google-libphonenumber": "^3.2.33",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "influx": "^5.9.3",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "io-ts": "^2.2.20",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.1",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "merge-yaml": "^1.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "morphism": "^1.12.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "sendwithus": "^6.0.2",
    "sinon-express-mock": "^2.2.1",
    "squel": "^5.12.2",
    "stripe": "^7.14.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-enum-util": "^4.0.2",
    "twilio": "^4.15.0",
    "uuid": "^9.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/bunyan": "^1.8.8",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/fast-json-stable-stringify": "^2.0.0",
    "@types/glob": "^8.1.0",
    "@types/google-libphonenumber": "^7.4.25",
    "@types/helmet": "0.0.48",
    "@types/http-errors": "^2.0.1",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.10.2",
    "@types/sinon-express-mock": "^1.3.9",
    "@types/stripe": "^7.13.11",
    "@types/swagger-jsdoc": "6.0.1",
