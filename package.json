{
  "name": "flo-public-gateway",
  "version": "1.0.0",
  "description": "Flo Public Gateway",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/flotechnologies/flo-public-gateway"
  },
  "scripts": {
    "copy-docs": "shx rm -rf dist/docs/*.yaml dist/docs/*/ && copyfiles docs/**/*.yaml dist",
    "build": "npm run build-ts && npm run tslint && npm run copy-docs",
    "build-ts": "tsc",
    "debug": "npm run build && node --inspect=0.0.0.0 dist/app.js",
    "serve": "npm run build && node dist/app.js",
    "start": "npm run serve",
    "test": "jest --colors --coverage --verbose",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch": "npm run copy-docs && tsc-watch --onSuccess 'node ./dist/app.js' --onFailure 'echo Compilation Failed'",
    "watch-debug": "npm run copy-docs && tsc-watch --onSuccess 'node --inspect=0.0.0.0 ./dist/app.js' --onFailure 'echo Compilation Failed'",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "author": "Flo Technologies",
  "dependencies": {
    "aws-sdk": "^2.437.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-basic-auth": "^1.2.0",
    "helmet": "^3.16.0",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "io-ts": "^1.8.5",
    "lodash": "^4.17.11",
    "morphism": "^1.11.1",
    "node-rdkafka": "^2.6.1",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.13",
    "squel": "^5.12.2",
    "stripe": "^6.31.2",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.2",
    "ts-enum-util": "^4.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.17.0",
    "@types/bunyan": "^1.8.6",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.43",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.0",
    "@types/pg": "^7.4.14",
    "@types/stripe": "^6.25.14",
    "@types/swagger-jsdoc": "0.0.2",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/uuid": "^3.4.4",
    "copyfiles": "^2.1.0",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.1",
    "tsc-watch": "^2.1.2",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.2"
  }
}
