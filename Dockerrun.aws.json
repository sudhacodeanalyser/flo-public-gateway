{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "098786959887.dkr.ecr.us-west-2.amazonaws.com/flo-hivemq:latest",
      "essential": true,
      "memoryReservation": 512,
      "ulimits": [
        {
          "softLimit": 65536,
          "hardLimit": 65536,
          "name": "nofile"
        }
      ],
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    }
  ]
}