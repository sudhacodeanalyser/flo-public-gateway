{
  "AWSEBDockerrunVersion": 2,
  "authentication": {
    "bucket": "flocloud-config",
    "key": "docker/.dockercfg"
  },
  "containerDefinitions": [
    {
      "name": "app",
      "image": "registry.gitlab.com/flotechnologies/flo-public-gateway:latest",
      "essential": true,
      "memoryReservation": 256,
      "ulimits": [
        {
          "softLimit": 65536,
          "hardLimit": 65536,
          "name": "nofile"
        }
      ],
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        },
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    }
  ]
}
